//1. Queue reversal

#include <iostream>
#include<queue>
#include<stack>
using namespace std;

int main()
{
    stack <int> s;
    queue<int> q;
    q.push(1);
    q.push(2);
    q.push(3);
    q.push(4);
   
    //remove from queue and add to stack
    while (!q.empty()) {
        int element = q.front();
        s.push(element);
        q.pop();
    }

    //remove from stack and add to queue
    while (!s.empty()) {
        int element2 = s.top();
        q.push(element2);
        s.pop();
    }

    //print the elements in queue after reversing(after using pop q is empty)
    while (!q.empty()) {
        cout << q.front() << " ";
        q.pop();
    }
    

    return 0;
}
