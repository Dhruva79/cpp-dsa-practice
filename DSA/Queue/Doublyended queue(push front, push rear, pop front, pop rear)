//stl: doubly ended queue (deque)

#include <iostream>
#include<queue>
using namespace std;


int main()
{
	deque <int> d;

	d.push_front(5);
	d.push_front(6);
	d.push_back(7);
	d.push_back(8);

	cout << d.front() << endl; //6
	cout << d.back() << endl;  //8

	d.pop_back();
	d.pop_back();

	cout << d.front() << endl;
	cout << d.back() << endl;

	d.pop_back();
	d.pop_back();

	if (d.empty()) {
		cout << "queue empty" << endl;
	}
	else {
		cout << d.back();
	}

	return 0;
}
