#include<iostream>
#include<queue>
using namespace std;

int main() {

	//create queue
	queue <int> q;

	//stl: push, pop, front, size, empty

	q.push(1);
	q.push(10);
	q.push(100);

	q.pop();
	cout << "size of queue is " << q.size() << endl;

	if (q.empty()) {
		cout << "q is empty";
	}
	else {
		cout << "Queue is not empty" << endl;
	}
	cout << q.front();

	return 0;
}
