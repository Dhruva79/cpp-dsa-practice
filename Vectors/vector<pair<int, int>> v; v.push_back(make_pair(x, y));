#include<iostream>
#include<vector>
 
using namespace std;

int main() {
	int n;
	cin >> n;
	cout << endl;

	vector<pair<int, int>> v;

	for (int i = 0; i < n; i++) {
		int x, y;
		cin >> x >> y;
		v.push_back(make_pair(x, y));

	}
	for (auto a : v) {
		cout << a.first << a.second;
		cout << endl;
	}
	return 0;
}
